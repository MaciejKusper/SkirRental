<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="skirental.controllers.ReturnController">
    <children>
        <HBox prefHeight="71.0" prefWidth="600.0">
            <children>
                <Label prefHeight="21.0" prefWidth="150.0" text="%order.button.orderNumber">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="orderNumber" prefHeight="21.0" prefWidth="30.0" text="-">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
            <TextField fx:id="takeOrder">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
                <Button fx:id="searchOrderButton" mnemonicParsing="false" onAction="#searchOrder" text="%order.button.searchOrder">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Label prefHeight="21.0" prefWidth="191.0" text="%button.customer">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
            <Label fx:id="customerLabel" text="-">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            </children>
        </HBox>
        <HBox prefHeight="71.0" prefWidth="600.0">
            <children>
                <Label prefHeight="17.0" prefWidth="150.0" text="%order.label.addItem">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <TextField fx:id="addItemsTextField" onAction="#addItemsListener">
                    <HBox.margin>
                        <Insets bottom="10.0" left="50.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </TextField>
                <Button fx:id="insertItem" mnemonicParsing="false" onAction="#addNewItem" text="%button.add">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <TableView fx:id="orderTableView" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="rfidTableColumn" prefWidth="125.0" text="%items.label.external_id" />
                <TableColumn fx:id="typeTabelColumn" prefWidth="125.0" text="%items.label.type" />
                <TableColumn fx:id="priceTableColumn" prefWidth="125.0" text="%items.label.price" />
                <TableColumn fx:id="sizeTableColumn" prefWidth="100.0" text="%items.label.size" />
                <TableColumn fx:id="descriptionTableColumn" minWidth="75.0" prefWidth="250.0" text="%items.label.description" />
            </columns>
        </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="21.0" prefWidth="175.0" text="%order.label.time">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="nuberHourLable" prefHeight="31.0" prefWidth="153.0" text="0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Label prefHeight="21.0" prefWidth="175.0" text="%order.label.sum">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="orderSum" layoutX="20.0" layoutY="20.0" prefHeight="21.0" prefWidth="60.0" text="0.00">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
                <Button fx:id="calculate" mnemonicParsing="false" onAction="#calculateButton" text="%button.calculate">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button fx:id="returnButton" mnemonicParsing="false" onAction="#saveOrder" text="%button.return">
                    <HBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</VBox>
